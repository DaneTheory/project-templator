{
  "name": "project-templator",
  "version": "0.9.0",
  "description": "Generate a project based on a directory of template files and corresponding template values",
  "main": "dist/src",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "generate",
    "project",
    "template",
    "directory",
    "boilerplate",
    "sao",
    "lass",
    "comptroller"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianmandrup/project-templator.git"
  },
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "contributors": [
    "Aiham Hammami <aiham@aiham.net>",
    "Kristian Mandrup <kmandrup@gmail.com>"
  ],
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "deepmerge": "^2.0.1",
    "ect": "^0.5.9",
    "escape-string-regexp": "^1.0.5",
    "find-derived": "^0.1.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "recursive-readdir": "^2.2.2",
    "run-sandboxed": "^0.1.1"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.3",
    "@types/escape-string-regexp": "0.0.32",
    "@types/fs-extra": "^5.0.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^9.4.6",
    "@types/recursive-readdir": "^2.2.0",
    "jest": "^22.4.2",
    "@types/jest": "^22.1.4",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2"
  }
}
